# Missing 'insert' key?
def_key "v"
  select_item
def_key "ctrl-v"
  reverse_selection
def_key "alt-v"
  select_range

# ...and 'delete'?
def_key "x"
  delete_playlist_items
def_key "x"
  delete_browser_items
def_key "x"
  delete_stored_playlist
def_key "X"
  toggle_crossfade
def_key "ctrl-x"
  set_crossfade

# vi keys
def_key "j"
    scroll_down
def_key "k"
    scroll_up
def_key "h"
    previous_column
def_key "l"
    next_column
def_key "."
    show_lyrics
def_key "n"
    next_found_item
def_key "N"
    previous_found_item
def_key "K"
  move_selected_items_up
def_key "J"
  move_selected_items_down

# Convenience binds.
def_key "+"
    show_clock
def_key "="
  volume_up
